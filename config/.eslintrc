{
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  plugins: [
    'vue'
  ],
  "globals": {
    "PROJECTNAME": true,
    "window": true
  },
  rules: {
    "no-console": "off",
    "vue/max-attributes-per-line": "off"
  }
}
